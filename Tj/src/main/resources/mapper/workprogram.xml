 		EMPL_NAME,EMPL_NUMBER,EMPL_AGE,EMPL_ADDR) VALUES(
 		#{empl.createdBy},
 		#{empl.createdTime},
 		#{empl.updatedby_},
 		#{empl.updatedTime},
 		#{empl.emplNo},
 		#{empl.emplName},
