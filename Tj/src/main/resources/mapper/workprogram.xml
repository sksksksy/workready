 		insert into day_records(REVISION,CREATED_BY,CREATED_TIME,UPDATED_BYID,UPDATED_TIME,PROG_NO,ADDR,DAY_ID,NOTE_PAY)
 		values(
 		#{dayR.revision},
 		#{dayR.createdBy},
 		#{dayR.createdTime},
 		#{dayR.updatedByid},
 		#{dayR.updatedTime},
 		#{dayR.progNo},
