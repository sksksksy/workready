 		UPDATED_TIME,PROG_NO,PROG_NAME,PROG_ADDR,PROG_MAN,
 		PROG_DAYS,PROG_PAY,PROG_HAS_PAY,PROG_ADD_PAY,
 		PROG_NOPAY,PROG_STATUS,CUCYCD,PROG_IINFOS)
 		VALUES(
 		#{workp.revision},
 		#{workp.createdBy},
 		#{workp.createdTime},
 		#{workp.updatedBy},
 		#{workp.updatedTime},
 		#{workp.progNo},
 		#{workp.progName},
 		#{workp.progAddr},
 		#{workp.progMan},
 		#{workp.progDyas},
 		#{workp.progPay},
 		#{workp.progHasPay},
 		#{workp.progAddPay},
 		#{workp.progNopay},
 		#{workp.progStatus},
 		#{workp.cucycd},
 		#{workp.progIinfos}
 		);
 	</insert>
